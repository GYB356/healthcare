# .env file for HealthcareSync application

# API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_MOCK_API=true

# Feature Flags
REACT_APP_ENABLE_SOCIAL_LOGIN=false
REACT_APP_ENABLE_EMAIL_VERIFICATION=true

# App Information
REACT_APP_VERSION=1.0.0
REACT_APP_COPYRIGHT_YEAR=2025

# Development Settings
DISABLE_ESLINT_PLUGIN=true
GENERATE_SOURCEMAP=false

REACT_APP_JWT_EXPIRY=1d
REACT_APP_AUTH_STORAGE_KEY=roofing_tracker_auth

NODE_ENV=development
PORT=5000
MONGODB_URI=mongodb://localhost:27017/roofing-tracker
JWT_SECRET=your_jwt_secret_key_here

TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_API_KEY=your_twilio_api_key
TWILIO_API_SECRET=your_twilio_api_secret